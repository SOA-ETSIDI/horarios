;; then enter the text in that file's own buffer.

git <- function(x)
{
#    withProgress(message = "Actualizando repositorio ...",{
    
        system2("git", "add .")
#        incProgress(1/5)
        system2("git",
                paste("commit -m '", x, "'"))
#        incProgress(2/5)
        system2("git", "push")
#        incProgress(1)
#    })
}
